package de.tudarmstadt.kom.carsimulator.speed;

public class SpeedDataRecord {
	private int _speedLimit;
	private int _duration;
	private String _points;
	
	public SpeedDataRecord(int pSpeedLimit, int pDuration, String pPoints) {
		_speedLimit = pSpeedLimit;
		_duration = pDuration;
		_points = pPoints;
	}

	public int getDuration() {
		return _duration;
	}
	
	public String getPoints() {
		return _points;
	}
	
	public void setPoints(String pPoints) {
		_points = pPoints;
	}
	
	public int getSpeedLimit() {
		return _speedLimit;
	}
	
	public void setDuration(int pDuration) {
		_duration = pDuration;
	}
	
	public void setSpeedLimit(int pSpeedLimit) {
		_speedLimit = pSpeedLimit;
	}
}
